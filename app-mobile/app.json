{
  "expo": {
    "name": "airport-agent",
    "slug": "airport-agent",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "icon": "./assets/adaptive-icon.png",

    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },

    "assetBundlePatterns": [
      "**/*"
    ],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.airportagent.publisher"
    },

    "web": {
      "favicon": "./assets/adaptive-icon.png"
    },

    "plugins": []
  }
}
